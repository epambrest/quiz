@model Teams.Models.ProgramCodeQuestionViewModel

@{
    ViewBag.Title = " ProgramCodeQuestion creating";
}

<form asp-action="CreateProgramQuestion" asp-controller="ProgramTest">
    <div class="form-group">
        <label asp-for="Text" class="control-label">Question text</label>
<textarea type="text" asp-for="Text" class="form-control"></textarea>
    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-light" />
    </div>
</form>